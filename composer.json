{
    "name": "woocommerce/woo-starter",
    "description": "An opinionated command line to to create the basic structure of a WooCommerce Extension.",
    "type": "cli-app",
    "require": {
        "symfony/console": "^5.4",
        "php": "^7.4 | ^8",
        "twig/twig": "^3.0",
        "lucatume/di52": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "bin": [
        "src/woo-starter.php"
    ],
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "WooStarter\\": "src/"
        }
    }
}
